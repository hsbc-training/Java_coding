
public abstract class Worker {
	private String name;
	private int salary;
	public Worker(String name, int salary) {
		super();
		this.name = name;
		this.salary = salary;
	}
	
	abstract int pay(int hours);

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getSalary() {
		return salary;
	}

	public void setSalary(int salary) {
		this.salary = salary;
	}
}
