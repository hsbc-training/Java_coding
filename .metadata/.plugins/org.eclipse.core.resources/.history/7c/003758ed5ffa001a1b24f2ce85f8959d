package com.hsbc.LabExercise42;

import java.util.List;

public class Registration extends Courses{

	public Registration(String courseName, String empName, char courseType, char courseMethod) {
		super(courseName, empName, courseType, courseMethod);
		// TODO Auto-generated constructor stub
	}

	public void register(List<Courses> list,Courses c) {
		// TODO Auto-generated method stub
		list.add(c);
	}

	@Override
	void deregister() {
		// TODO Auto-generated method stub
		
	}

}
