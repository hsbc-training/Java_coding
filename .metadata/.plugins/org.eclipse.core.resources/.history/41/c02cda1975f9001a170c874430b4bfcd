import java.util.LinkedList;

public class LinkedListExample {
	public static void main(String[] args) {
		LinkedList<String> ll = new LinkedList<String>();
		
		ll.add("Vishnu");
		ll.add("Gopal");
		ll.add("Thobbi");
		
		System.out.println(ll);
		
		ll.add(3, "Fresher");
		
		System.out.println(ll);
	}
}
