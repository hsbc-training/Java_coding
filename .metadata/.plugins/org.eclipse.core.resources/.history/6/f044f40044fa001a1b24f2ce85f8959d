import java.util.ArrayList;
import java.util.List;

public class Company {
	
		List<Worker> worker = new ArrayList<>();
		
		void salaryCalculate(){
			
			for(int i=0;i<3;i++){
				SalariedWorker s = new SalariedWorker();
				worker.add(s);
			}
			
			for(int i=0;i<5;i++){
				DailyWorker d = new DailyWorker();
				worker.add(d);
			}
		}
	

}
