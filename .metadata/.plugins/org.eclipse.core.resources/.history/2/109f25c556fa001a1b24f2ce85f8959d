package com.hsbc.LabExercise42;

public abstract class Courses {
	private String courseName;
	private float courseFee;
	private char courseMethod;
	private char courseType;
	
	abstract void register();
	abstract void deregister();
	abstract void courseList();
	
	public Courses(String courseName, float courseFee, char courseMethod, char courseType) {
		super();
		this.courseName = courseName;
		this.courseFee = courseFee;
		this.courseMethod = courseMethod;
		this.courseType = courseType;
	}
}
